<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="bookDao" class="com.example.dao.impl.BookDaoImpl">
        <property name="array">
            <array>
                <value>100</value>
                <value>200</value>
                <value>300</value>
            </array>
        </property>

        <property name="list">
            <list>
                <value>100</value>
                <value>200</value>
                <value>300</value>
            </list>
        </property>

        <property name="set">
            <set>
                <value>hi</value>
                <value>hei</value>
                <value>ha</value>
            </set>
        </property>

        <property name="map">
            <map>
                <entry key="country" value="CH"/>
                <entry key="province" value="Beijing"/>
                <entry key="city" value="Beijing"/>
            </map>
        </property>
        <property name="properties">
            <props>
                <prop key="country">CH</prop>
                <prop key="province">Beijing</prop>
                <prop key="city">Beijing</prop>
            </props>
        </property>

    </bean>
</beans>